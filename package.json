    "jest": "^25.5.4",
    "jstransformer-handlebars": "^1.1.0",
    "jstransformer-qejs": "^0.2.0",
    "lint-staged": "^8.2.1",
    "metalsmith": "^2.3.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "inputformat-to-jstransformer": "^1.4.0",
